{
  "name": "truefans-geoconnect",
  "version": "1.0.0",
  "private": true,
  "description": "Live Music Discovery & Artist Support with Advanced Geolocation",
  "keywords": [
    "nextjs",
    "supabase",
    "geolocation",
    "music",
    "donations",
    "artists"
  ],
  "author": "TrueFans Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/truefans/geoconnect.git"
  },
  "scripts": {
    "dev": "NODE_OPTIONS=\"--max-old-space-size=16384\" next dev -H 0.0.0.0 --experimental-https",
    "dev:ssl": "NODE_OPTIONS=\"--max-old-space-size=16384\" next dev -H 0.0.0.0 --experimental-https --experimental-https-key ./ssl/localhost.key --experimental-https-cert ./ssl/localhost.crt",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" next build --no-lint",
    "postbuild": "cp .next/static/css/*.css public/coming-soon.css",
    "start": "next start -H 0.0.0.0",
    "start:ssl": "next start -H 0.0.0.0 --experimental-https --experimental-https-key ./ssl/localhost.key --experimental-https-cert ./ssl/localhost.crt",
    "lint": "next lint",
    "clean:next": "rm -rf .next",
    "clean:deps": "npx rimraf node_modules package-lock.json",
    "clean:all": "npm run clean:next && npm run clean:deps",
    "fresh": "npm run clean:all && npm install && npm run dev",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "supabase:generate-types": "supabase gen types typescript --linked > lib/database.types.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:push": "supabase db push",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules/.cache .next/cache",
    "test:standalone": "./scripts/test-standalone.sh",
    "build:docker": "./scripts/build-docker.sh"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.446.0",
    "next": "13.5.1",
    "next-themes": "^0.3.0",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
